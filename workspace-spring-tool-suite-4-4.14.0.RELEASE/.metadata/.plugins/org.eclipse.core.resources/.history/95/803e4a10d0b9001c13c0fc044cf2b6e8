package hello;

public class helloMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 25;
		
		if(i>20) {
			System.out.println("Hello World");

		}
		else {
			System.out.println("i is not greater than 20");
		}
		
		while (i>20) {
			System.out.println("I like dogs");
			i--;
		}
			
	}

}
